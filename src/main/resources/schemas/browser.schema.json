{
  "init_browser": {
    "type": "object",
    "properties": {
      "session": {
        "type": "string",
        "description": "Session name to identify driver instance (default: mcp_session)"
      },
      "headless": {
        "type": "boolean",
        "description": "Run browser in headless mode"
      },
      "reuse": {
        "type": "boolean",
        "description": "Reuse existing driver if available"
      }
    },
    "additionalProperties": false
  },
  "navigate": {
    "type": "object",
    "properties": {
      "session": {
        "type": "string",
        "description": "Session name (default: mcp_session)"
      },
      "url": {
        "type": "string",
        "description": "URL to navigate to. Ex: https://example.com"
      }
    },
    "required": [
      "url"
    ],
    "additionalProperties": false
  },
  "get_page_title": {
    "type": "object",
    "properties": {
      "session": {
        "type": "string",
        "description": "Session name (default: mcp_session)"
      }
    },
    "additionalProperties": false
  },
  "close": {
    "type": "object",
    "properties": {
      "session": {
        "type": "string",
        "description": "Session name (default: mcp_session)"
      }
    },
    "additionalProperties": false
  },
  "execute_script": {
    "type": "object",
    "properties": {
      "session": {
        "type": "string",
        "description": "Session name (default: mcp_session)"
      },
      "script": {
        "type": "string",
        "description": "JavaScript code to execute in browser context"
      }
    },
    "required": [
      "script"
    ],
    "additionalProperties": false
  }
}
